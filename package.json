{
  "name": "@antd-templater/template-3.x",
  "description": "后台管理系统模版 - 基于 Vue3.x Ant Design Vue 组件库",
  "homepage": "https://github.com/antd-templater/template-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT License",
  "version": "4.2.1",
  "keywords": [
    "front-end",
    "ant-design-vue",
    "antd-template-vue3.x"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "cypress": "cypress open --component",
    "build": "vue-tsc --noEmit && vite build",
    "eslint": "npx eslint --fix --quiet src/**/*{.vue,.tsx,.ts}",
    "release": "release-it",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antd-templater/library-3.x": "^4.3.10",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "js-simpler": "^1.6.2",
    "md5": "^2.3.0",
    "msw": "^2.3.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.12.1",
    "vue": "^3.4.30",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.4.0",
    "vue-types": "^5.1.2"
  },
  "devDependencies": {
    "@antd-templater/eslint-config": "^1.0.4",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/config-inspector": "^0.5.0",
    "@eslint/js": "^9.5.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@stylistic/eslint-plugin": "^2.2.2",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.14.8",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/vue-ls": "^3.2.7",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.12.0",
    "cz-message-helper": "^1.3.1",
    "darkreader": "^4.9.86",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "release-it": "^17.4.0",
    "rollup": "^4.18.0",
    "terser": "^5.31.1",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.6.0",
    "volar-service-prettyhtml": "^0.0.54",
    "vue-tsc": "^2.0.22"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
