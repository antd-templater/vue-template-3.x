{
  "name": "@antd-templater/vue-template-3.x",
  "description": "后台管理系统模版 - 基于 Vue3 + Ant Design Vue4.x 组件库",
  "homepage": "https://github.com/antd-templater/vue-template-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT License",
  "version": "4.3.1",
  "keywords": [
    "front-end",
    "ant-design-vue",
    "antd-template-vue3.x"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vitest --ui",
    "cypress": "cypress open --component",
    "build": "vue-tsc --noEmit && vite build",
    "eslint": "npx eslint --fix --quiet src/**/*{.vue,.tsx,.ts}",
    "release": "release-it",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antd-templater/library-3.x": "^4.3.20",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.7.8",
    "dayjs": "^1.11.13",
    "js-simpler": "^1.6.2",
    "md5": "^2.3.0",
    "msw": "^2.6.6",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.6",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-ls": "^4.2.0",
    "vue-router": "^4.5.0",
    "vue-types": "^5.1.3"
  },
  "devDependencies": {
    "@antd-templater/eslint-config": "^1.0.7",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/config-inspector": "^0.5.6",
    "@eslint/js": "^9.15.0",
    "@release-it/conventional-changelog": "^9.0.3",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.10.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@types/vue-ls": "^3.2.7",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/coverage-v8": "^2.1.6",
    "@vitest/ui": "^2.1.6",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^13.16.0",
    "cz-message-helper": "^2.0.0",
    "darkreader": "^4.9.96",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "less": "^4.2.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "release-it": "^17.10.0",
    "rollup": "^4.27.4",
    "terser": "^5.36.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^6.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^2.1.6",
    "volar-service-prettyhtml": "^0.0.62",
    "vue-tsc": "^2.1.10"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=9.12.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "pnpm@9.12.3"
